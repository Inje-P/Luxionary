<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/tur_background"> <!-- Ripple Effect 색상 -->

    <!-- 배경색 -->
    <item android:id="@+id/ripple_tur_full">
